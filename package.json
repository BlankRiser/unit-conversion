{
	"name": "@ram/unit-conversion",
	"private": "false",
	"version": "0.0.1",
	"description": "unit conversion sdk",
	"license": "ISC",
	"author": "Ram Shankar Choudhary",
	"type": "module",
	"scripts": {
		"build": "rolldown --config ./rolldown.config.js",
		"dev": "bun --watch run src/index.ts",
		"lint": "biome check src",
		"lint:fix": "biome check --fix src",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@types/node": "^24.1.0",
		"@vitest/ui": "3.2.4",
		"rolldown": "1.0.0-beta.30",
		"rolldown-plugin-dts": "0.15.0",
		"typescript": "^5.9.2",
		"vitest": "3.2.4"
	}
}
